{
  "ignoreCase": false,
  "tokenizer": {
    "root": [
      {
        "include": "@whitespace"
      },
      ["(#.*)", "comment"],
      ["\\b(world)\\b", "variable"],
      ["(\u0027[A-Za-z_\\-0-9]+\\.[A-Za-z_\\-0-9]+)", "support.function"],
      ["(\u0027[A-Za-z_\\-0-9]+)", "number"],
      ["\\b([a-z_0-9][A-Za-z_\\-0-9]*)\\b", "identifier"],
      ["(\\\"[^\\\"\\n]*\\\")", "string"],
      ["(\\@\\@(.|\\n)*\\@\\@)", "string"]
    ],
    "whitespace": [
      ["(\\s+)", "white"],
      ["\\@\\@", "string", "@stringLiteral"],
      ["\\(", "keyword", "@keyword1"],
      ["\\(", "keyword", "@quotedIdentifier1"],
      ["\\(", "keyword", "@functionIdentifier1"]
    ],
    "stringLiteral": [
      [
        "#py",
        {
          "nextEmbedded": "python",
          "goBack": 3,
          "next": "@embedded",
          "token": "string.python"
        }
      ],
      [
        "\\\/\\\/js",
        {
          "nextEmbedded": "javascript",
          "goBack": 4,
          "next": "@embedded",
          "token": "string.js"
        }
      ],
      [
        "{",
        {
          "nextEmbedded": "json",
          "goBack": 1,
          "next": "@embedded",
          "token": "string.json"
        }
      ],
      ["[^\\@\\@]", "string"],
      ["\\@\\@", "string", "@pop"],
      ["\\@\\@", "string"]
    ],
    "embedded": [
      [
        "([^@]|^)([@]{4})*[@]{2}([@]([^@]|$)|[^@]|$)",
        {
          "nextEmbedded": "@pop",
          "next": "@pop",
          "token": "@rematch"
        }
      ]
    ],
    "keyword1": [
      [
        "(set_package_version|override_library|package|library|declare|return|lambda|import|export|quote|block|let)",
        "keyword",
        "@pop"
      ]
    ],
    "quotedIdentifier1": [["([A-Za-z_\\-0-9]+!)", "string.tablepath", "@pop"]],
    "functionIdentifier1": [["([A-Z][A-Za-z_\\-0-9]*)", "support.function", "@pop"]]
  }
}
